   "source": [
    "k=int(input(\"Enter The Number which Have To Print Multiplication Tabel:\\n\"))\n",
    "print(\"*#\"*7+\" \"+\"Multiplication Table Of {k}\".format(k=k)+\" \"+\"*#\"*7)\n",
    "i=1\n",
    "while(i<13):\n",
    "    print(k,\"X\" ,i,\"=\",k*i)\n",
    "    i=i+1"
   ]
